{
  "expo": {
    "name": "HR App",
    "slug": "hr-system-mobile",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/osam-32x32.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.revotech.hrapp",
      "supportsTablet": true
    },
    "android": {
      "package": "com.revotech.hrapp"
    },
    "androidStatusBar": {
      "backgroundColor": "#fcfcfc"
    }
  }
